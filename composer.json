{
    "name": "autn/laravel-schema",
    "description": "The Laravel schema package.",
    "keywords": ["framework", "laravel", "schema", "database"],
    "support": {
        "issues": "https://github.com/autn/laravel-schema/issues",
        "source": "https://github.com/autn/laravel-schema"
    },
    "authors": [
        {
            "name": "Au Truong Ngoc",
            "email": "autk08@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.*"
    },
    "require-dev": {
        "laravel/laravel": "5.2.*",
        "illuminate/support": "5.2.*",
        "orchestra/testbench": "~3.2",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Autn\\Schema\\":"packages/Schema/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"

        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer install --optimize-autoloader"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
